{
  "name": "jspf-md-demo",
  "version": "1.0.3",
  "description": "FIX 4.4 market data demo using quick fix data dictionary",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "generate": "cd node_modules/jspurefix/dist && node jsfix-cmd \"--dict=../../data/FIX44-MD.xml\" \"--compile\" \"--output=../../src/types/\"",
    "replay": "cd node_modules/jspurefix/dist && node jsfix-cmd \"--dict=../../data/FIX44-MD.xml\" \"--fix=../../data/example/fix.txt\" \"--objects\" --delimiter=\"|\"",
    "tokens": "cd node_modules/jspurefix/dist && node jsfix-cmd \"--dict=../../data/FIX44-MD.xml\" \"--fix=../../data/example/fix.txt\" \"--tokens\" --delimiter=\"|\"",
    "md-app": "cd dist/app && node app --port 3000 --server ../../data/session/test-acceptor.json",
    "help": "cd dist/app && node app --help"
  },
  "keywords": [
    "quickfix",
    "fix",
    "jspurefix"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^9.4.1",
    "jspurefix": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "typings": "^2.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.186",
    "@types/sax": "^1.2.4",
    "standard": "^17.0.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.3",
    "@typescript-eslint/eslint-plugin-tslint": "^5.35.1",
    "eslint": "^8.22.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-jquery": "^1.5.1",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-node": "^11.1.0",
    "madge": "^5.0.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
